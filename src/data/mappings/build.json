    "archive": [
      "7z",
      "apk",
      "bz2",
      "crate",
      "deb",
      "dmg",
      "egg",
      "flatpak",
      "gem",
      "gz",
      "iso",
      "jar",
      "lz4",
      "msi",
      "nupkg",
      "pkg",
      "rar",
      "rpm",
      "snap",
      "tar",
      "taz",
      "tbz",
      "tbz2",
      "tgz",
      "tlz",
      "txz",
      "tzo",
      "whl",
      "xz",
      "zip",
      "zst"
    ],
    "binary": [
      "a",
      "beam",
      "class",
      "dll",
      "dylib",
      "elc",
      "exe",
      "lib",
      "o",
      "obj",
      "pyc",
      "pyo",
      "so",
      "spv",
      "wasm"
    ],
    "debug": ["dsym", "pdb"],
    "linker": ["ilk"],
    "lock": ["lock", "lockb"],
    "map": ["map"],
    "script": [
      "awk",
      "bash",
      "bat",
      "cmd",
      "csh",
      "fish",
      "ksh",
      "nu",
      "nushell",
      "ps1",
      "psm1",
      "psrc",
      "sed",
      "sh",
      "tcl",
      "tcsh",
      "vbs",
      "vsh",
      "zsh"
    ],
    "search": ["code-search"]
  },
  "fileNames": {
    "ignore": [
      ".bazelignore",
      ".cvsignore",
      ".forgejo-ignore",
      ".gcloudignore",
      ".helmignore",
      ".hgignore",
      ".markdownlintignore",
      ".swiftlintignore",
      ".vercelignore",
