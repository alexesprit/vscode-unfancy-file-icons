        "names": [
          "docker",
          "eslint",
          "git",
          "npm",
          "nyc",
          "prettier",
          "remark",
          "stylelint",
          "vscode"
        ],
        "templates": [".${name}ignore"]
      }
    ],
    "lock": [
      ".vpkg-lock.json",
      "bun.lock",
      "bun.lockb",
      "cargo.lock",
      "composer.lock",
      "deno.lock",
      "flake.lock",
      "gemfile.lock",
      "go.sum",
      "gradle.lockfile",
      "mix.lock",
      "npm-shrinkwrap.json",
      "package-lock.json",
      "packages.lock.json",
      "pdm.lock",
      "pipfile.lock",
      "pnpm-lock.json",
      "pnpm-lock.yaml",
      "poetry.lock",
      "pubspec.lock",
      "shrinkwrap.yaml",
      "uv.lock",
      "yarn.lock"
    ],
    "script": ["gradlew"]
  }
